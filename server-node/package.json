{
  "name": "authentik-temporal-service",
  "version": "1.0.0",
  "description": "Authentik backend service with Temporal client integration",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "debug": "tsx --inspect src/index.ts",
    "debug-brk": "tsx --inspect-brk src/index.ts",
    "debug-built": "node --inspect dist/index.js",
    "debug-built-brk": "node --inspect-brk dist/index.js",
    "worker": "tsx src/temporal/worker.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@temporalio/activity": "^1.13.0",
    "@temporalio/client": "^1.13.0",
    "@temporalio/common": "^1.13.0",
    "@temporalio/worker": "^1.13.0",
    "@temporalio/workflow": "^1.13.0",
    "@types/cookie-parser": "^1.4.9",
    "better-auth": "^1.3.8",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "postgres": "^3.4.3",
    "resend": "^3.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "temporal",
    "express",
    "typescript",
    "authentik"
  ],
  "author": "",
  "license": "MIT"
}
