{
  "name": "authentik-temporal-server",
  "version": "1.0.0",
  "description": "Authentik Temporal Server with GRPC bridge and email integrations",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "npm run proto:generate && tsc",
    "start": "node dist/index.js",
    "proto:generate": "grpc_tools_node_protoc --js_out=import_style=commonjs,binary:./src/generated --grpc_out=grpc_js:./src/generated --plugin=protoc-gen-grpc=./node_modules/.bin/grpc_tools_node_protoc_plugin -I ./proto ./proto/*.proto && grpc_tools_node_protoc --plugin=protoc-gen-ts=./node_modules/.bin/protoc-gen-ts --ts_out=grpc_js:./src/generated -I ./proto ./proto/*.proto",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.14",
    "@grpc/proto-loader": "^0.7.10",
    "@temporalio/activity": "^1.10.0",
    "@temporalio/client": "^1.10.0",
    "@temporalio/common": "^1.10.0",
    "@temporalio/worker": "^1.10.0",
    "@temporalio/workflow": "^1.10.0",
    "@types/uuid": "^9.0.7",
    "dotenv": "^16.3.1",
    "grpc-tools": "^1.12.4",
    "pino": "^9.9.4",
    "pino-pretty": "^13.1.1",
    "postmark": "^4.0.2",
    "resend": "^2.1.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "temporal",
    "grpc",
    "typescript",
    "authentik",
    "newsletters",
    "resend",
    "postmark"
  ],
  "author": "",
  "license": "MIT"
}
